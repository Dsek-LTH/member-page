{
  "name": "core",
  "version": "1.0.0",
  "description": "API for data related to backend core.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && copyfiles -u 1 src/**/*.graphql dist",
    "dev": "concurrently npm:dev:*",
    "dev:start": "cd src && ts-node-dev --respawn --transpile-only --watch . index.ts",
    "dev:generate": "graphql-codegen --config codegen.yml --watch",
    "start": "node dist/index.js",
    "test": "NODE_ENV=test mocha --exit -r ts-node/register tests/**/*.spec.ts",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/federation": "^0.20.4",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-resolvers": "^2.4.2",
    "@keycloak/keycloak-admin-client": "^15.0.2",
    "@types/chai": "^4.2.14",
    "@types/chai-spies": "^1.0.3",
    "@types/deep-equal-in-any-order": "^1.0.1",
    "@types/lodash": "^4.14.175",
    "@types/mocha": "^8.0.4",
    "@types/mock-knex": "^0.4.3",
    "@types/node-cron": "^3.0.0",
    "apollo-datasource": "^0.7.2",
    "apollo-server": "^2.25.3",
    "apollo-server-caching": "^0.5.2",
    "apollo-server-errors": "^3.2.0",
    "apollo-server-testing": "^2.19.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "chonky": "^2.3.2",
    "concurrently": "^6.4.0",
    "copyfiles": "^2.4.0",
    "deep-equal-in-any-order": "^1.1.15",
    "dsek-shared": "file:../../shared",
    "expo-server-sdk": "^3.6.0",
    "knex": "^0.21.12",
    "mocha": "^10.1.0",
    "lodash": "^4.17.21",
    "meilisearch": "^0.24.0",
    "minio": "^7.0.18",
    "node-cron": "^3.0.0",
    "pg": "^8.6.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/minio": "^7.0.14"
  }
}
